<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>
   <extension-point id="reExecuteScript" name="Re-execute History Result Entry" schema="schema/reExecuteScript.exsd"/>
   <extension-point id="resultSetOutputter" name="resultSetOutputter" schema="schema/resultSetOutputter.exsd"/>
   <extension-point id="resultSetViewer" name="Additional Viewers for ResultSets" schema="schema/resultSetViewer.exsd"/>
   <extension-point id="resultListener" name="Result Listener" schema="schema/resultListener.exsd"/>
   <extension
         point="org.eclipse.ui.views">
      <view
            category="org.eclipse.datatools"
            class="org.eclipse.datatools.sqltools.result.internal.ui.view.ResultsView"
            icon="icons/sqlresult.gif"
            id="org.eclipse.datatools.sqltools.result.resultView"
            name="%SQLResultsView.name"/>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            class="org.eclipse.datatools.sqltools.result.internal.preference.SQLResultsViewPage"
            id="org.eclipse.datatools.sqltools.result.displayOption"
            name="%SQLResultsView.display.options"
            category="org.eclipse.datatools.connectivity.sqm.core.internal.ui.preferences.data"/>
      <page
            class="org.eclipse.datatools.sqltools.result.internal.preference.ExportFormatPreferencePage"
            id="org.eclipse.datatools.sqltools.result.exportFormat"
            name="%SQLResultsView.export.options"
            category="org.eclipse.datatools.sqltools.result.displayOption"/>
      <page
            class="org.eclipse.datatools.sqltools.result.internal.preference.ResultHistoryPage"
            id="org.eclipse.datatools.sqltools.result.resultHistory"
            name="%SQLResultsView.history.page"
            category="org.eclipse.datatools.sqltools.result.displayOption"/>
      <page
            class="org.eclipse.datatools.sqltools.result.internal.preference.ResultSetViewerPreferencePage"
            id="org.eclipse.datatools.sqltools.result.resultSetViewerPreference"
            name="%SQLResultsView.resultsetviewer.page"
            category="org.eclipse.datatools.sqltools.result.displayOption"/>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <category
            id="org.eclipse.datatools.sqltools.result.category"
            name="%SQLResultsView.full.name"/>
      <command
            categoryId="org.eclipse.datatools.sqltools.result.category"
            id="org.eclipse.datatools.sqltools.result.removeInstance"
            name="%RemoveResultAction.command.name"/>
      <command
            categoryId="org.eclipse.datatools.sqltools.result.category"
            id="org.eclipse.datatools.sqltools.result.removeAllInstances"
            name="%RemoveAllResultAction.command.name"/>
      <command
            categoryId="org.eclipse.datatools.sqltools.result.category"
            id="org.eclipse.datatools.sqltools.result.terminate"
            name="%TerminateInstanceAction.command.name"/>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="org.eclipse.datatools.sqltools.result.removeInstance"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M1+DEL"/>
      <key
            commandId="org.eclipse.datatools.sqltools.result.removeAllInstances"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M2+DEL"/>
      <key
            commandId="org.eclipse.datatools.sqltools.result.terminate"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M1+BREAK"/>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer class="org.eclipse.datatools.sqltools.result.internal.preference.ResultsViewPreferenceInitializer"/>
   </extension>
   <extension
         point="org.eclipse.datatools.sqltools.result.resultSetOutputter">
      <Ouputter
            class="org.eclipse.datatools.sqltools.result.internal.export.TextOutputter"
            fileExtension="txt"
            supportDelimiter="true"
            supportXMLResult="true"
            typeId="org.eclipse.datatools.sqltools.result.textOutputter"
            typeName="%Outputter.plain.text">
      </Ouputter>
      <Ouputter
            class="org.eclipse.datatools.sqltools.result.internal.export.XMLOutputter"
            fileExtension="xml"
            supportDelimiter="false"
            supportXMLResult="true"
            typeId="org.eclipse.datatools.sqltools.result.xmloutputter"
            typeName="%Outputter.xml">
      </Ouputter>
      <Ouputter
            class="org.eclipse.datatools.sqltools.result.internal.export.HTMLOutputter"
            extensionFilter="*.html;*.htm"
            fileExtension="html"
            supportDelimiter="false"
            supportXMLResult="false"
            typeId="org.eclipse.datatools.sqltools.result.htmloutputter"
            typeName="%Outputter.html">
      </Ouputter>
      <Ouputter
            class="org.eclipse.datatools.sqltools.result.internal.export.CSVOutputter"
            fileExtension="csv"
            supportDelimiter="false"
            supportXMLResult="false"
            typeId="org.eclipse.datatools.sqltools.result.csvoutputter"
            typeName="%Outputter.csv">
      </Ouputter>
   </extension>

</plugin>
